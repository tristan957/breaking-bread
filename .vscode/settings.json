{
	"editor.insertSpaces": false,
	"eslint.enable": false,
	"[html]": {
		"editor.detectIndentation": true,
		"editor.tabSize": 2,
	},
	"[json]": {
		"editor.detectIndentation": true,
		"editor.tabSize": 2,
		"editor.wordWrap": "on",
	},
	"[jsonc]": {
		"editor.detectIndentation": true,
		"editor.tabSize": 2,
		"editor.wordWrap": "on",
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": true,
		},
		"editor.formatOnSave": true,
	},
	"[typescriptreact]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": true,
		},
		"editor.formatOnSave": true,
	},
	"tslint.autoFixOnSave": true,
	"tslint.exclude": "node_modules/**",
	"tslint.jsEnable": false,
	"tslint.packageManager": "yarn",
	"files.associations": {
		"*bashrc": "shellscript",
		"maven.config": "plaintext",
		"cctype": "cpp",
		"clocale": "cpp",
		"cstdarg": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"cwchar": "cpp",
		"cwctype": "cpp",
		"array": "cpp",
		"*.tcc": "cpp",
		"cmath": "cpp",
		"cstdint": "cpp",
		"unordered_map": "cpp",
		"vector": "cpp",
		"exception": "cpp",
		"fstream": "cpp",
		"initializer_list": "cpp",
		"iosfwd": "cpp",
		"iostream": "cpp",
		"istream": "cpp",
		"limits": "cpp",
		"new": "cpp",
		"optional": "cpp",
		"ostream": "cpp",
		"sstream": "cpp",
		"stdexcept": "cpp",
		"streambuf": "cpp",
		"string_view": "cpp",
		"system_error": "cpp",
		"cinttypes": "cpp",
		"type_traits": "cpp",
		"tuple": "cpp",
		"typeinfo": "cpp",
		"utility": "cpp",
		"__config": "cpp",
		"__nullptr": "cpp"
	},
}
